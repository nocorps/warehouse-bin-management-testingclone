# Firebase Configuration
# Copy this file to .env.local and fill in your Firebase project details

# Firebase Config - Get these from Firebase Console -> Project Settings -> General -> Your apps
REACT_APP_FIREBASE_API_KEY=your-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789012
REACT_APP_FIREBASE_APP_ID=1:123456789012:web:abcdef123456

# App Configuration
REACT_APP_APP_NAME=Warehouse Bin Management
REACT_APP_VERSION=1.0.0

# Development Settings
REACT_APP_DEV_TOOLS_ENABLED=true
REACT_APP_MOCK_DATA_ENABLED=true
